{
  "name": "ng-sm-ms",
  "version": "0.4.4",
  "description": "使用SM.MS图床的API写的桌面应用程序",
  "author": "Ajanuw <ajanuw1995@gmail.com> (https://github.com/januwA)",
  "main": "main.js",
  "license": "MIT",
  "scripts": {
    "ng": "ng",
    "start": "ng serve --host 0.0.0.0  --disable-host-check",
    "build": "ng build",
    "prod": "ng build --prod",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "e-start": "electron .",
    "e-dev": "npm run build && electron .",
    "e-build": "npm run build && electron-builder -w",
    "e-win": "npm run prod && electron-builder -w",
    "e-mac": "npm run prod && electron-builder -m",
    "e-linux": "npm run prod && electron-builder -l",
    "e-mwl": "npm run prod && electron-builder -mwl"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~9.0.3",
    "@angular/cdk": "^9.1.1",
    "@angular/common": "~9.0.3",
    "@angular/compiler": "~9.0.3",
    "@angular/core": "~9.0.3",
    "@angular/forms": "~9.0.3",
    "@angular/material": "^9.1.1",
    "@angular/platform-browser": "~9.0.3",
    "@angular/platform-browser-dynamic": "~9.0.3",
    "@angular/router": "~9.0.3",
    "ajanuw-waterfall.js": "^0.1.3",
    "ajanuw-widgets-angular": "^0.1.1",
    "clipboard": "^2.0.6",
    "localforage": "^1.7.3",
    "rxjs": "~6.5.4",
    "tslib": "^1.10.0",
    "zone.js": "~0.10.2"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.900.4",
    "@angular/cli": "~9.0.4",
    "@angular/compiler-cli": "~9.0.3",
    "@angular/language-service": "~9.0.3",
    "@types/node": "^12.11.1",
    "@types/jasmine": "~3.5.0",
    "@types/jasminewd2": "~2.0.3",
    "codelyzer": "^5.1.2",
    "jasmine-core": "~3.5.0",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~4.3.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "~2.1.0",
    "karma-jasmine": "~2.0.1",
    "karma-jasmine-html-reporter": "^1.4.2",
    "protractor": "~5.4.3",
    "ts-node": "~8.3.0",
    "tslint": "~5.18.0",
    "typescript": "~3.7.5",
    "electron": "^9.4.0",
    "electron-builder": "^22.3.6",
    "@types/clipboard": "^2.0.1"
  },
  "build": {
    "appId": "com.ajanuw.sm-ms",
    "productName": "sm ms",
    "copyright": "ajanuw",
    "directories": {
      "output": "release"
    },
    "mac": {
      "category": "public.app-category.developer-tools",
      "target": "dmg",
      "icon": "docs/assets/icon@512x512.png"
    },
    "linux": {
      "category": "Network",
      "icon": "docs/assets"
    },
    "win": {
      "target": [
        {
          "target": "nsis",
          "arch": [
            "x64",
            "ia32"
          ]
        }
      ],
      "icon": "docs/assets/icon@512x512.png"
    },
    "nsis": {
      "oneClick": false,
      "allowToChangeInstallationDirectory": true
    }
  }
}
