{
  "name": "sm-ms",
  "version": "0.4.4",
  "description": "使用SM.MS图床的API写的桌面应用程序",
  "author": "Ajanuw <ajanuw1995@gmail.com> (https://github.com/januwA)",
  "main": "main.js",
  "license": "MIT",
  "scripts": {
    "ng": "ng",
    "start": "ng serve --host 0.0.0.0  --disable-host-check",
    "build": "ng build",
    "prod": "ng build --prod",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "e-start": "electron .",
    "e-dev": "npm run build && electron .",
    "e-build": "npm run build && electron-builder -w",
    "e-win": "npm run prod && electron-builder -w",
    "e-mac": "npm run prod && electron-builder -m",
    "e-linux": "npm run prod && electron-builder -l",
    "e-mwl": "npm run prod && electron-builder -mwl"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~8.2.14",
    "@angular/cdk": "~8.2.3",
    "@angular/common": "~8.2.14",
    "@angular/compiler": "~8.2.14",
    "@angular/core": "~8.2.14",
    "@angular/forms": "~8.2.14",
    "@angular/material": "8.2.3",
    "@angular/platform-browser": "~8.2.14",
    "@angular/platform-browser-dynamic": "~8.2.14",
    "@angular/router": "~8.2.14",
    "ajanuw-waterfall.js": "^0.1.3",
    "ajanuw-widgets-angular": "^0.1.0",
    "clipboard": "^2.0.4",
    "core-js": "^3.6.4",
    "global": "^4.4.0",
    "hammerjs": "^2.0.8",
    "localforage": "^1.7.3",
    "rxjs": "^6.5.4",
    "tslib": "^1.11.0",
    "zone.js": "~0.9.1"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^0.803.25",
    "@angular/cli": "^8.3.25",
    "@angular/compiler-cli": "~8.2.14",
    "@angular/language-service": "~8.2.14",
    "@types/clipboard": "^2.0.1",
    "@types/jasmine": "^3.5.7",
    "@types/jasminewd2": "~2.0.3",
    "@types/lodash": "^4.14.141",
    "@types/node": "^12.12.28",
    "codelyzer": "^5.0.1",
    "electron": "^7.1.13",
    "electron-builder": "^21.2.0",
    "jasmine-core": "~3.5.0",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~4.4.1",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "~2.1.1",
    "karma-jasmine": "~2.0.1",
    "karma-jasmine-html-reporter": "^1.5.2",
    "protractor": "^5.4.3",
    "ts-node": "~8.5.4",
    "tslint": "~5.20.1",
    "typescript": "~3.5.0"
  },
  "build": {
    "appId": "com.ajanuw.sm-ms",
    "productName": "sm ms",
    "copyright": "ajanuw",
    "directories": {
      "output": "release"
    },
    "mac": {
      "category": "public.app-category.developer-tools",
      "target": "dmg",
      "icon": "docs/assets/icon@512x512.png"
    },
    "linux": {
      "category": "Network",
      "icon": "docs/assets"
    },
    "win": {
      "target": [
        {
          "target": "nsis",
          "arch": [
            "x64",
            "ia32"
          ]
        }
      ],
      "icon": "docs/assets/icon@512x512.png"
    },
    "nsis": {
      "oneClick": false,
      "allowToChangeInstallationDirectory": true
    }
  }
}